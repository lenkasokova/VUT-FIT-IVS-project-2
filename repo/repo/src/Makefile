ZIP = xbinov00_xlapes02_xsokov01_xsmeka16
DOC = doxygen_documentation
RM = rm -f
all:

pack:
	cd ../../.. && zip -r $(ZIP).zip $(ZIP)/

clean:
	cd ../ && rm -rf $(ZIP).zip  	# rm archive
	$(RM) -r $(DOC)					# rm documentation generated by Doxygen

doc: Doxyfile
	rm -drf ../../doc/
	doxygen Doxyfile
	mv -f doc/ ../../

test:
	cd calculator/ && python3 test_math_lib.py # run tests

latex:
	pdflatex dokumentace.tex # compile .tex to pdf
	mv dokumentace.pdf ../   # mv pdf to out
	@$(MAKE) -s latex-clean  # rm all {aux,dvi,log,ps,out} files

latex-clean:
	$(RM) dokumentace.{aux,dvi,log,ps,out}

run:
	cd calculator/ && python3 GUI.py

profile:
	cd calculator && python3 -m cProfile -o ../../profiling/vystup.prof stddev.py


